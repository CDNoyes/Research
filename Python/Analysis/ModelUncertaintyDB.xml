<?xml version='1.0' encoding='UTF-8'?>
<ModelDB 
    xmlns="imuQ" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="imuQ file:/Users/ldpeters/sw/lims/trunk/install/include/imuQ/XML/Schema/ModelDB.xsd" 
    name="MarsEDL">
    <multiphysicsModel name="MarsEDL">
        <parameters>
                
<!-- Vehicle Parameters -->
            <variable name="CD" value="0" units="" description="Drag Coefficient Error">
                <variability mapping="replace">
                    <aleatory>
                        <uniform lowerBound="-0.10" upperBound="+0.10"/>
                    </aleatory>
                </variability>
            </variable>
            <variable name="CL" value="0" units="" description="Lift Coefficient Error">
                <variability mapping="replace">
                    <aleatory>
                        <uniform lowerBound="-0.10" upperBound="+0.10"/>
                    </aleatory>
                </variability>
            </variable>

<!-- Environment Parameters -->
            <variable name="density0" value="0.0158" units="kg/m^3" description="rho0 used in Exponential Atmosphere Model">
                <variability mapping="scale">
                    <aleatory>
                        <normal mean="0" stdDev="0.0333" lowerBound="-0.20" upperBound="+0.20"/>
                    </aleatory>
                </variability>
            </variable>
            <variable name="densitySH" value="9354.5" units="m" description="Scale Height Used in Exponential Atmosphere Model">
                <variability mapping="scale">
                    <aleatory>
                        <uniform lowerBound="-0.05" upperBound="+0.05"/>
                    </aleatory>
                </variability>
            </variable>           
            
<!-- Initial States: latitude, longitude, altitude -->
            <variable name="latitude" value="0" units="deg" description="Latitude perturbation in degrees">
<!--                     <variability mapping="add">
                        <aleatory>
                            <uniform lowerBound="-8.6e-4" upperBound="+8.6e-4"/>
                        </aleatory>
                    </variability> -->
            </variable>
            <variable name="longitude" value="0" units="deg" description="Longitude perturbation in radians">
<!--                 <variability mapping="add">
                    <aleatory>
                        <uniform lowerBound="-8.6e-4" upperBound="+8.6e-4"/>
                    </aleatory>
                </variability> -->
            </variable>
            <variable name="altitude" value="0" units="m" description="Initial altitude perturbation in meters">
<!--                 <variability mapping="add">
                    <aleatory>
                        <uniform lowerBound="-10" upperBound="+10"/>
                    </aleatory>
                </variability> -->
            </variable>
        </parameters>    

    </multiphysicsModel>
    <analyses>
        <importProtocolDB dir="Analyses" db="AnalysisProtocol.xml"/>
    </analyses>
</ModelDB>