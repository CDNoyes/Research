<?xml version='1.0' encoding='utf-8'?>
<ProtocolDB 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="imuQ file:/Users/ldpeters/sw/LIMS/trunk/install/include/imuQ/XML/Schema/ModelDB.xsd" 
    xmlns="imuQ">
    <qmuProtocol
        name="TimeSimulation"
        destination="Analysis">
        <dshellProtocol 
            name="TimeSimulation" 
            inputFile="GuidedEntry.py"
            commandLinePrefix="/bin/nice -n 10 "
            commandLineArguments="">
            <auxDir     path="Results"/>
            <auxFile    path="Baseline/run_dshell_protocol.pbs"/>
            <postScript path="Baseline/postProcess_results.py"/>
            <symLink    path="EntryGuidance"/>
            <symLink    path="Utils"/>
            <responses>
                <responseFunction name="radius"             units="m"/>
                <responseFunction name="longitude"          units="deg"/>
                <responseFunction name="latitude"           units="deg"/>
                <responseFunction name="velocity"           units="m/s"/>
                <responseFunction name="fpa"                units="deg"/>
                <responseFunction name="heading"            units="deg"/>
            </responses>
        </dshellProtocol>
        
        <dakotaProtocol 
            name="LHS" 
            inputFile="Analysis/Methods/LHS/dakota.xml"
            destination="Methods/LHS"
            evaluationConcurrency="5">
            <auxFile    path="Analysis/Methods/LHS/run_dakota_protocol.pbs"/>
            <postScript path="Analysis/Methods/LHS/postProcess_results.py"/>
        </dakotaProtocol>
        <dakotaProtocol 
        <dakotaProtocol 
            name="ParameterStudy" 
            inputFile="Analysis/Methods/ParameterStudy/dakota.xml"
            evaluationConcurrency="10">
            <auxFile    path="Analysis/Methods/ParameterStudy/run_dakota_protocol.pbs"/>
            <postScript path="Analysis/Methods/ParameterStudy/postProcess_results.py"/>
        </dakotaProtocol>
        <dakotaProtocol 
            name="GlobalSensitivity" 
            inputFile="Analysis/Methods/GlobalSensitivity/dakota.xml"
            evaluationConcurrency="5">
            <auxFile    path="Analysis/Methods/Global_Sensitivity/run_dakota_protocol.pbs"/>
            <postScript path="Analysis/Methods/Global_Sensitivity/postProcess_results.py"/>
        </dakotaProtocol>
        <dakotaProtocol 
            name="QMC" 
            inputFile="Analysis/Methods/009-Quasi_Monte_Carlo_UQ/dakota.xml"
            evaluationConcurrency="5">
            <auxFile    path="Analysis/Methods/QMC/run_dakota_protocol.pbs"/>
            <postScript path="Analysis/Methods/QMC/postProcess_results.py"/>
        </dakotaProtocol>
    </qmuProtocol>
</ProtocolDB>
