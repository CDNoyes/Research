<?xml version='1.0' encoding='utf-8'?>
<ProtocolDB 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="imuQ file:/Users/ldpeters/sw/LIMS/trunk/install/include/imuQ/XML/Schema/ModelDB.xsd" 
    xmlns="imuQ">
    <qmuProtocol
        name="TimeSimulation"
        destination="Analyses/TimeSimulation">
        <pythonProtocol 
            name="TimeSimulation" 
            inputFile="IMUQ_PROJECT_SOURCE_DIR/GuidedEntry.py">
            <auxDir     path="Results"/>
            <auxFile    path="Analyses/TimeSimulation/Baseline/run_python_protocol.pbs"/>
            <postScript path="Analyses/TimeSimulation/Baseline/postProcess_results.py"/>
            <symLink    path="IMUQ_PROJECT_SOURCE_DIR/EntryGuidance"/>
            <symLink    path="IMUQ_PROJECT_SOURCE_DIR/Utils"/>
            <responses>
                <responseFunction name="radius"             units="m"/>
                <responseFunction name="longitude"          units="deg"/>
                <responseFunction name="latitude"           units="deg"/>
                <responseFunction name="velocity"           units="m/s"/>
                <responseFunction name="fpa"                units="deg"/>
                <responseFunction name="heading"            units="deg"/>
            </responses>
        </pythonProtocol>
        <dakotaProtocol 
            name="LHS" 
            inputFile="Analyses/TimeSimulation/Methods/LHS/dakota.xml"
            evaluationConcurrency="5">
            <auxFile    path="Analyses/TimeSimulation/Methods/LHS/run_dakota_protocol.pbs"/>
            <postScript path="Analyses/TimeSimulation/Methods/LHS/postProcess_results.py"/>
        </dakotaProtocol>
    </qmuProtocol>
</ProtocolDB>

